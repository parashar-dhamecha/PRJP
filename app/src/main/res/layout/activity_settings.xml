<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Settings">
    <androidx.cardview.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="60dp" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            android:layout_marginStart="8dp" android:layout_marginEnd="8dp"
            android:id="@+id/cardView2"
            android:layout_marginTop="24dp" android:layout_marginBottom="16dp"
            app:layout_constraintBottom_toTopOf="@+id/cardView">
        <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

            <TextView
                    android:text="@string/My_account"
                    android:layout_width="259dp"
                    android:layout_height="23dp"
                    android:layout_centerVertical="true"
                    android:id="@+id/textView16"
                    android:layout_alignParentBottom="true" android:layout_marginBottom="18dp"
                    android:layout_toEndOf="@+id/imageView15" android:layout_marginStart="23dp"
                    android:layout_marginTop="18dp" android:layout_alignParentTop="true" android:textSize="18sp"
                    android:textStyle="bold"
                    android:layout_toStartOf="@+id/imageView18" android:layout_marginEnd="17dp"
                    android:textAlignment="viewStart"/>
            <ImageView
                    android:layout_width="30dp"
                    android:layout_height="30dp" app:srcCompat="@drawable/right" android:id="@+id/imageView18"
                    android:layout_alignParentEnd="true" android:layout_marginEnd="18dp"
                    android:layout_alignParentBottom="true" android:layout_marginBottom="12dp"
                    android:layout_marginTop="12dp" android:layout_alignParentTop="true"
                    tools:ignore="ContentDescription"/>
            <ImageView
                    android:layout_width="30dp"
                    android:layout_height="30dp" app:srcCompat="@drawable/profile" tools:layout_editor_absoluteY="360dp"
                    tools:layout_editor_absoluteX="134dp" android:id="@+id/imageView15"
                    android:layout_alignParentStart="true" android:layout_marginStart="18dp"
                    android:layout_alignParentBottom="true" android:layout_marginBottom="12dp"
                    android:layout_marginTop="12dp" android:layout_alignParentTop="true"
                    tools:ignore="ContentDescription"/>
        </RelativeLayout>
    </androidx.cardview.widget.CardView>
    <androidx.cardview.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:id="@+id/cardView"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintHorizontal_bias="1.0" android:layout_marginBottom="16dp"
            app:layout_constraintBottom_toTopOf="@+id/cardView3"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/cardView2">
        <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent" android:id="@+id/relativeLayout">

            <TextView
                    android:text="@string/Change_Password"
                    android:layout_width="263dp"
                    android:textStyle="bold"
                    android:textSize="18sp"
                    android:layout_centerVertical="true"
                    android:layout_height="22dp" android:id="@+id/textView21"
                    android:layout_marginTop="18dp" android:layout_alignParentTop="true"
                    android:layout_toStartOf="@+id/imageView17" android:layout_marginEnd="13dp"
                    android:layout_alignParentBottom="true" android:layout_marginBottom="18dp"
                    android:layout_toEndOf="@+id/imageView13" android:layout_marginStart="23dp"/>
            <ImageView
                    android:layout_width="30dp"
                    tools:ignore="ContentDescription"
                    android:layout_height="30dp" app:srcCompat="@drawable/right" android:id="@+id/imageView17"
                    android:layout_alignParentEnd="true" android:layout_marginEnd="18dp"
                    android:layout_alignParentBottom="true" android:layout_marginBottom="12dp"
                    android:layout_marginTop="12dp" android:layout_alignParentTop="true"/>
            <ImageView
                    android:layout_width="30dp"
                    android:layout_height="30dp" app:srcCompat="@drawable/keyy"
                    tools:ignore="ContentDescription"
                    tools:layout_editor_absoluteY="132dp"
                    android:id="@+id/imageView13"
                    android:layout_marginStart="18dp" app:layout_constraintEnd_toEndOf="parent"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentBottom="true" android:layout_marginBottom="12dp"
                    android:layout_marginTop="12dp" android:layout_alignParentTop="true"
            />
        </RelativeLayout>
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="60dp"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" android:layout_marginEnd="8dp" app:layout_constraintEnd_toEndOf="parent"
            android:id="@+id/cardView3"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/cardView"
            android:layout_marginBottom="391dp" app:layout_constraintBottom_toBottomOf="parent">
        <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

            <ImageView
                    android:layout_width="30dp"
                    android:layout_height="30dp" app:srcCompat="@drawable/right" android:id="@+id/imageView19"
                    android:layout_alignParentBottom="true" android:layout_marginBottom="13dp"
                    android:layout_alignParentEnd="true" android:layout_marginEnd="18dp" android:layout_marginTop="12dp"
                    android:layout_alignParentTop="true"
                    tools:ignore="ContentDescription"/>
            <TextView
                    android:text="@string/Logout"
                    android:layout_width="255dp"
                    android:layout_centerVertical="true"
                    android:layout_height="24dp" android:id="@+id/textView19" android:layout_alignParentBottom="true"
                    android:layout_marginBottom="18dp"
                    android:layout_toEndOf="@+id/imageView14"
                    android:layout_marginStart="23dp" android:layout_toStartOf="@+id/imageView19"
                    android:layout_marginEnd="21dp" android:layout_marginTop="18dp"
                    android:layout_alignParentTop="true" android:textSize="18sp" android:textStyle="bold"/>
            <ImageView
                    android:layout_width="30dp"
                    android:layout_height="30dp" app:srcCompat="@drawable/logout" tools:layout_editor_absoluteY="490dp"
                    tools:ignore="ContentDescription"
                    tools:layout_editor_absoluteX="64dp"
                    android:id="@+id/imageView14" android:layout_alignParentBottom="true"
                    android:layout_marginBottom="18dp" android:layout_alignParentStart="true"
                    android:layout_marginStart="18dp" android:layout_marginTop="12dp"
                    android:layout_alignParentTop="true"/>
        </RelativeLayout>
    </androidx.cardview.widget.CardView>

</androidx.constraintlayout.widget.ConstraintLayout>