<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   xmlns:tools="http://schemas.android.com/tools"
                                                   android:orientation="horizontal"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="match_parent">

    <EditText
            android:layout_width="195dp"
            android:layout_height="37dp"
            android:ems="10"
            android:hint="Search"
            android:drawableLeft="@drawable/searchicon"
            android:id="@+id/editText4"
            android:layout_marginStart="16dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginTop="40dp"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginEnd="16dp"
            app:layout_constraintEnd_toStartOf="@+id/button" app:layout_constraintHorizontal_bias="0.0"/>
    <Button
            android:layout_width="40dp"
            android:layout_height="34dp"
            android:background="@drawable/addusers"
            android:id="@+id/button"
            android:layout_marginTop="40dp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintEnd_toStartOf="@+id/button2"
            android:layout_marginEnd="16dp"/>
    <Button
            android:layout_width="40dp"
            android:layout_height="34dp"
            android:background="@drawable/group"
            android:id="@+id/button2"
            android:layout_marginTop="40dp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintEnd_toStartOf="@+id/button5"
            android:layout_marginEnd="16dp"/>
    <Button
            android:layout_width="40dp"
            android:layout_height="34dp"
            android:background="@drawable/files"
            android:id="@+id/button5"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="16dp" android:layout_marginTop="40dp" app:layout_constraintTop_toTopOf="parent"/>
    <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/recyclerView"
            android:layout_width="395dp"
            android:layout_height="510dp" tools:ignore="MissingConstraints"/>

</androidx.constraintlayout.widget.ConstraintLayout>