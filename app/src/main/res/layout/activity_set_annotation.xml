<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".SetAnnotation">
    <ImageView android:layout_width="match_parent" android:layout_height="0dp"
               android:layout_margin="@dimen/dp50"
               android:id="@+id/annotationiv"
               app:layout_constraintStart_toStartOf="parent"
               app:layout_constraintEnd_toEndOf="parent"
               android:layout_marginBottom="534dp" app:layout_constraintBottom_toTopOf="@+id/buttonannotation"
               android:contentDescription="@string/todo"
               app:layout_constraintTop_toTopOf="parent" android:scaleType="matrix"/>
    <Button android:layout_width="fill_parent" android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="@dimen/dp50"
            android:padding="@dimen/dp10"
            android:layout_marginRight="@dimen/dp50"
            android:text="@string/anoteButton"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginBottom="@dimen/dp40" app:layout_constraintBottom_toBottomOf="parent"
            android:id="@+id/buttonannotation" app:layout_constraintHorizontal_bias="0.0"/>

</androidx.constraintlayout.widget.ConstraintLayout>