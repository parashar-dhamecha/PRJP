<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:id="@+id/rootview"
>
    <TextView android:layout_width="101dp" android:layout_height="23dp"
              android:id="@+id/xycoordinates"
              app:layout_constraintStart_toStartOf="@+id/button3"
              app:layout_constraintTop_toTopOf="parent"/>
    <Spinner android:layout_width="@dimen/dp280"
             android:layout_height="43dp"
             android:tooltipText="@string/selectsigners"

             app:layout_constraintTop_toBottomOf="@+id/xycoordinates"
             app:layout_constraintStart_toStartOf="@+id/Relativelid" android:layout_marginStart="8dp"
             app:layout_constraintEnd_toStartOf="@+id/button4" android:layout_marginEnd="8dp"/>
    <Button android:layout_width="@dimen/dp40"
            android:layout_height="@dimen/dp50"
            android:text="X"
            android:padding="@dimen/dp0"
            android:layout_centerVertical="true"
            android:background="@drawable/edit_round2"
            android:id="@+id/button3"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginTop="252dp"
            app:layout_constraintTop_toBottomOf="@+id/xycoordinates" android:layout_marginStart="4dp"
            android:layout_marginEnd="8dp" app:layout_constraintEnd_toStartOf="@+id/Relativelid"
            app:layout_constraintHorizontal_bias="1.0"/>
    <Button android:layout_width="@dimen/dp40"
            android:layout_height="@dimen/dp50"
            android:text="X"
            android:padding="@dimen/dp0"
            android:layout_centerVertical="true"
            android:background="@drawable/edit_round2"
            android:id="@+id/button4"
            app:layout_constraintStart_toEndOf="@+id/Relativelid" android:layout_marginStart="8dp"
            android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="@+id/button3"/>
    <RelativeLayout android:layout_width="wrap_content" android:layout_height="wrap_content"
                    android:layout_marginTop="96dp"
                    app:layout_constraintTop_toTopOf="parent"
                    android:layout_marginStart="8dp"
                    app:layout_constraintStart_toStartOf="parent" android:layout_marginEnd="8dp"
                    app:layout_constraintEnd_toEndOf="parent" app:layout_constraintHorizontal_bias="0.495"
                    android:id="@+id/Relativelid">
        <ImageView android:layout_width="760px" android:layout_height="1078.0487px"
                   android:layout_gravity="center"
                   android:layout_centerVertical="true"
                   android:id="@+id/previewdocid"
        />
    </RelativeLayout>
</androidx.constraintlayout.widget.ConstraintLayout>